@section FileUploadStyle {
    <link rel="stylesheet" href="~/css/fileupload.css">
}
<form id="insertNewCyleForm">
    <div class="text-dark vh-100 p-5">
        <p class="text-center fs-2 fw-bold">Insert New Cycle</p>
        <div class="frame">
            <div class="center">
                <div class="title">
                    <h1 id="filename">Drop file to upload</h1>
                </div>
                <div class="dropzone" id="dropzone">
                    <img src="http://100dayscss.com/codepen/upload.svg" class="upload-icon" />
                    <input name="excelFile" type="file" class="upload-input" accept=".xlsx" id="excelFile" required />
                </div>
                <button type="submit" class="btn fileupload-btn d-flex align-items-center justify-content-center gap-2">
                    Upload file
                </button>
            </div>
        </div>
        <div class="container-fluid bg-white p-0 shadow-lg flex-column justify-content-between mt-5"
            style="border-radius: 10px;max-height:600px;overflow:auto;">
            <table class="table table-striped table-borderless mb-0" style="border-collapse: collapse;">
                <tbody id="response">

                </tbody>
            </table>
        </div>

    </div>
</form>


<script>
    let Result = @Html.Raw(Json.Serialize(Model));
    var applicationRoot = "@Url.Content("~")";
</script>
<script src="~/js/User/user.js"></script>
<script src="~/js/fileupload.js"></script>
<script src="~/js/User/insertNewCycle.js"></script>